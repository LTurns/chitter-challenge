<% @makerall.each do |info| %>
<h3>Welcome, <%= info.name %> <% end %></h3>

<ul>
  <% @peeps.each do |peep| %>
   <%= peep.username %>
   <%= peep.peep %>
   <%= peep.datetime %>
  <% end %>
</ul>


<form action="/add_peep" type="get">
<input type="submit" value="Add Peep!">
</form>

<form action="/" type="get">
  <input type="submit" value="=logout">
</form>
